<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gh.clickgo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/circlegoapp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckSeats" />
        <activity android:name=".PaymentMethod" />
        <activity android:name=".Settings" />
        <activity android:name=".OurPartner" />
        <activity android:name=".seats"></activity>
        <activity android:name=".umair"></activity>
    </application>

</manifest>